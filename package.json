{
    "name": "protonio",
    "productName": "protonio",
    "version": "1.0.1",
    "description": "A quick access tool to execute scripts on desktop",
    "main": "src/index.js",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint src --color"
    },
    "keywords": [],
    "author": "Mohamed Oumoumad",
    "license": "MIT",
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "packageManager": "npm",
                "icon": "src/menu/icons/logo_full.ico",
                "executableName": "protonio"
            },
            "electronWinstallerConfig": {
                "name": "protonio"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "protonio"
            }
        }
    },
    "dependencies": {
        "auto-launch": "^5.0.5",
        "current-path": "^2.0.0",
        "electron-compile": "^6.4.3",
        "electron-context-menu": "^0.9.1",
        "electron-edge-js": "^8.3.8",
        "sweetalert": "^2.1.2",
        "tree-kill": "^1.2.1"
    },
    "devDependencies": {
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "electron": "^3.0.10",
        "electron-forge": "^4.3.0",
        "electron-prebuilt-compile": "1.7.9",
        "electron-squirrel-startup": "^1.0.0",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-react": "^7.11.1",
        "node-gyp": "^3.8.0"
    }
}
